page {
    background-color: #fff;
}

.swiperBanner {
    width: 100%;
    margin-top: 140rpx;
}

.swiperBanner swiper {
    width: 100%;
    height: 316rpx;
    transition: .3s;
}

.swiperBanner swiper .slide-nav {
    display: block;
    height: 100%;
    width: 100%;
}

.swiperBanner swiper .pic {
    width: 100%;
}

.box .content {
    color: #fff;
    font-size: 40rpx;
    font-weight: 900;
    letter-spacing: 5rpx;
    padding: 0;
    position: relative;
    background-color: #fff;
    border-radius: 0;
}

.box .content::after {
    border: 0;
    border-radius: 0;
}

.banner {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 40rpx 30rpx;
    letter-spacing: 2rpx;
}

.banner text:nth-child(1) {
    font-size: 40rpx;
    color: #0026ff;
    font-weight: 700;
    margin-bottom: 30rpx;
}

.banner text:nth-child(2) {
    font-size: 24rpx;
    color: #b4aeae;
}

.box_list {
    margin: 0 30rpx;
    display: flex;
    justify-content: space-between;
}

.box_list .content {
    width: 330rpx;
    height: 560rpx;
    border: 2rpx solid #eee;
    border-radius: 15rpx;
}

.box_list .content .top {
    height: 270rpx;
    border-bottom: 2rpx solid #eee;
}

.box_list .content .top,.list {
    margin: 0 40rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-items: center;
}

.box_list .content .top .box_pic {
    width: 140rpx;
    height: 140rpx;
}

.box_list .content .top .box_pic .pic {
    width: 100%;
    height: 100%;
}

.box_list .content .list {
    height: 220rpx;
    margin-top: 30rpx;
    font-size: 28rpx;
}

.box_list .content .list text {
    color: #ff8d3e;
}

.free {
    width: 224rpx;
    height: 70rpx;
    background-color: #1084ff;
    border-radius: 15rpx;
    margin: 40rpx auto;
    text-align: center;
    line-height: 70rpx;
    font-size: 34rpx;
    color: #fff;
}

.free .blue {
    width: 100%;
    height: 100%;
}

.public_one {
    width: 100%;
    height: 900rpx;
    padding: 0 30rpx;
    box-sizing: border-box;
    padding-top: 50rpx;
    background: url(data:image/jpeg;base64,) no-repeat;
    background-size: cover;
}

.public_one .title .name {
    font-size: 42rpx;
    color: #fff;
    font-weight: 700;
    text-align: center;
}

.public_one .title .info {
    font-size: 26rpx;
    color: #fff;
    margin: 30rpx 0;
    letter-spacing: 4rpx;
}

.public_one .public_list {
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
    position: relative;
}

.public_one .public_list .item {
    width: 330rpx;
    height: 226rpx;
    background-color: #fff;
    margin-bottom: 30rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 50rpx 40rpx;
    box-sizing: border-box;
}

.public_one .public_list .one,.three {
    border-radius: 10rpx 0 0 10rpx;
}

.public_one .public_list .two,.four {
    border-radius: 0 10rpx 10rpx 0;
}

.public_one .public_list .item .name {
    font-size: 30rpx;
    color: #282828;
    margin-bottom: 20rpx;
}

.public_one .public_list .item .info {
    font-size: 22rpx;
    color: #999;
}

.public_one .public_list .picture {
    width: 179rpx;
    height: 179rpx;
    background-color: #1870ec;
    border-radius: 50%;
    position: absolute;
    top: 48%;
    left: 50%;
    margin-left: -89.5rpx;
    margin-top: -89.5rpx;
}

.public_one .public_list .picture .pic image {
    width: 109rpx;
    height: 108rpx;
    margin-top: 20%;
    margin-left: 20%;
}

.public_one .box_get {
    width: 240rpx;
    height: 70rpx;
    background-color: #ffa326;
    text-align: center;
    line-height: 70rpx;
    color: #fff;
    font-size: 28rpx;
    margin: 30rpx auto;
}

.public_one .box_get .get {
    width: 100%;
    height: 100%;
}

.public_title {
    margin-top: 50rpx;
    padding: 0 32rpx;
}

.public_two {
    width: 100%;
    height: 440rpx;
    margin-top: -57rpx;
    padding-top: 175rpx;
    box-sizing: border-box;
    background: url(data:image/png;base64,) no-repeat;
    background-size: cover;
}

.public_two .public_title .name {
    font-size: 42rpx;
    font-weight: 700;
    color: #2a7efb;
    text-align: center;
}

.public_two .public_title .info {
    font-size: 26rpx;
    color: #999;
    margin-top: 18rpx;
}

.content_list {
    width: 100%;
    height: 506rpx;
    padding: 0 30rpx;
    box-sizing: border-box;
    margin-top: -50rpx;
    display: flex;
    justify-content: space-around;
    flex-wrap: wrap;
    align-items: center;
}

.content_list .item {
    width: 272rpx;
}

.content_list .item .name {
    font-size: 30rpx;
    color: #282828;
    margin: 26rpx 0 10rpx 0;
}

.content_list .item .info {
    font-size: 26rpx;
    color: #999;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
}

.public_three {
    width: 100%;
    height: 360rpx;
    margin-top: 76rpx;
    margin-bottom: 30rpx;
    color: #fff;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-around;
    background: url(data:image/jpeg;base64,) no-repeat;
    background-size: cover;
}

.public_three .title1 {
    font-size: 40rpx;
    font-weight: 700;
}

.public_three .title2 {
    font-size: 50rpx;
    font-weight: 700;
}