@font-face {
    font-family: iconfont;
    src: url(data:font/ttf;base64,) format("truetype");
}

.iconfont {
    font-family: iconfont!important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-baocuntupianzhonggaoban:before {
    content: "\e674";
}

.icon-riqi:before {
    content: "\e629";
}

.icon-shangchuantupian1:before {
    content: "\e68d";
}

.icon-jihuoma:before {
    content: "\e639";
}

.icon-weixinzhifu:before {
    content: "\e618";
}

.icon-weizhifu:before {
    content: "\e6f2";
}

.icon-yiwancheng:before {
    content: "\e60f";
}

.icon-shalou1:before {
    content: "\e632";
}

.icon-shangchuan:before {
    content: "\e631";
}

.icon-kefu:before {
    content: "\e608";
}

.icon-24gl-circle:before {
    content: "\ea97";
}

.icon-shalou:before {
    content: "\e605";
}

.box1 {
    background-color: #123dc7;
    width: 100%;
    height: 212rpx;
}

.data {
    position: absolute;
    top: 0;
    left: 0;
    margin: auto;
    width: 100%;
    margin-top: 120rpx!important;
    display: flex;
    flex-direction: column;
    padding-bottom: 40rpx;
}

.box5 {
    width: 100%;
    height: 106rpx;
    position: fixed;
    right: 0;
    bottom: 0;
    background-color: #fff;
    box-shadow: 0 0 4rpx 2rpx #dcdcdc;
}

.but {
    background-blend-mode: color-burn;
    width: 250rpx;
    font-weight: 400;
    font-size: 24rpx;
    color: #000;
    text-align: center;
}

.error_msg {
    margin-left: 15rpx;
}

.box4 {
    padding-top: 40rpx!important;
}

.sand {
    background-color: #ffab1a;
    width: 30rpx;
    height: 30rpx;
    border-radius: 50%;
}

.icon-shalou1,.icon-weizhifu,.icon-yiwancheng {
    margin: 0 28rpx 0 66rpx;
}

.xian>image {
    width: 250rpx;
    height: auto;
    min-height: 250rpx;
}

.button {
    border-radius: 10rpx;
    border: none;
    color: #fff;
    width: 166rpx;
    height: 58rpx;
    line-height: 58rpx!important;
    margin-right: 38rpx;
}

.xian {
    padding-left: 40rpx;
    border-left: #b6b9bf 1rpx dashed;
    margin-left: 16rpx;
}

.sand>image {
    width: 18rpx;
    top: 5rpx;
    left: 3rpx;
}

.box1>view {
    height: 122rpx;
}

.box2>view,.box3>view,.box6>view {
    margin-top: 30rpx;
}

.box2,.box3,.box4 {
    background-color: #fff;
    border-radius: 15rpx;
    box-shadow: 0 0 4rpx 2rpx #dcdcdc;
    padding: 1rpx 52rpx 34rpx 52rpx;
    width: 582rpx;
    margin: auto;
    margin-top: 27rpx;
}

.quick_r {
    background-color: #f55;
    color: #fff;
    letter-spacing: 4rpx;
    font-size: 32rpx;
}

.amend {
    background-color: #123dc7;
    color: #fff;
    letter-spacing: 4rpx;
    font-size: 32rpx;
    margin-top: 20rpx;
}